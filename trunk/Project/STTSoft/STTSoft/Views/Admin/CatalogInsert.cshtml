@{
    ViewBag.Title = "CatalogInsert";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@if ((string)Session["role"] == "A")
{
    <h2>CatalogInsert</h2>
    <form action="/Admin/CatalogInsert" method="POST">
        <table>
            <tr>
                <td>Name:
                </td>
                <td>
                    <input name="txtName" value="" /><br />
                </td>
            </tr>
            <tr>
                <td>
                    <button type="submit" class="btn btn-primary">
                        Submit</button>
                </td>
            </tr>
        </table>
    </form>
}
else
{
    <div class="row">
        <h2>Bạn không có quyền truy cập trang này.  <a href="@Url.Action("Index", "Home")">Click here</a></h2>
    </div>
}