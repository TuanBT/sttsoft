@model List<STTSoft.STTSoftService.BankDTO>
@{
    ViewBag.Title = "BankList";
}




@if ((string)Session["role"] == "A")
{
    <h2>BankList</h2>
    <div class="widget-content medias">
        <table class="table table-bordered ">
            <thead>
                <tr>
                    <th style="color: royalblue">Name</th>
                    <th style="color: royalblue">Money</th>
                    <th style="color: royalblue">Edit</th>
                    <th style="color: royalblue">Delete</th>
                </tr>
            </thead>
            @foreach (var bank in Model)
            {
                <tbody>
                    <tr>
                        <td>@bank.AccName</td>
                        <td>@bank.BanMoney VND</td>
                        <td><a href="/Admin/BankEdit?accName=@bank.AccName" class="btn btn-sm btn-danger"><i class="icon-pencil"></i></a></td>
                        <td><a href="/Admin/BankDelete?accName=@bank.AccName" class="btn btn-sm btn-info"><i class="icon-remove"></i></a></td>
                    </tr>
                </tbody>
            }
        </table>
    </div>
}
else
{
    <div class="row">
        <h2>Bạn không có quyền truy cập trang này.  <a href="@Url.Action("Index", "Home")">Click here</a></h2>
    </div>
}
