@{
    ViewBag.Title = "AccountEdit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>AccountEdit</h2>


@model STTSoft.Models.Account
    
<form action="/Admin/AccountEdit" method="POST">     
    Name: <input name="txtName" value="@Model.AccName"/><br/>
    Role: <input name="txtRole" value="@Model.AccRole"/><br/>
    Level: <input name="txtLevel" value="@Model.AccLevel"/><br/>
    Mail: <input name="txtMail" value="@Model.AccMail"/><br/>
    Phone: <input name="txtPhone" value="@Model.AccPhone"/><br/>
    <button type="submit">Submit</button>
</form>  